#!/bin/bash
mkdir css js
touch index.html ./css/styles.css ./js/main.js
echo "<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>TODO</title>
    <link rel="stylesheet" href="./css/styles.css">
</head>
<body>
    
    <script src="./js/main.js"></script>
</body>
</html>" >> index.html

echo "*{
    padding: 0;
    margin: 0;
    box-sizing: border-box;
}" >> ./css/styles.css